{
  "expo": {
    "name": "expo-51-imgly",
    "slug": "expo-51-imgly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raider.expo51imgly"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raider.expo51imgly"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.8.10"
          }
        }
      ],
      [
        "react-native-imglysdk",
        {
          "android": {
            "version": "10.9.0",
            "kspVersion": "1.8.10-1.0.9"
          }
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
